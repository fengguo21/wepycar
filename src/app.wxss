.tab{
    width:100%;
height:49px;
  display: block;
  position: relative;
  background: rgba(255,255,255,1);
  box-shadow: 0px 0px 6px 0px rgba(0,0,0,0.12);
  overflow: hidden;
}
.tab .addWrapper{
     position: absolute;
    height: 49px;
    width:49px;
    right: 0;
    top: 0;
    display: flex;
    align-items: center;
    justify-content: center;
}

.tab .add{
   
    height: 20px;
    width:20px;
   
   
    
}
.tab .logo{
   width:72px;
   height: 20px;
  display: block;
  margin: 15px auto;
  padding: 0;
    display: block;
   
}
.cu-list .cu-item{
    display: flex;
height:68px;
margin:0 16px;
border-bottom: 1rpx solid rgba(0, 0, 0, 0.1);
}
.cu-item .content{
    margin:14px 0;
font-size:16px;
font-family:PingFangSC-Medium;
font-weight:500;
color:rgba(64,64,64,1);
line-height:22px;
}
.cu-item .content .binded{
    font-size:13px;
font-family:SFNSText;
color:rgba(163,0,0,1);
line-height:16px;
}
.cu-item .content .notbinded{
    font-weight:400;
color:rgba(191,191,191,1);
line-height:18px;
}
.cu-avatar image{
    width:44px;
height:44px;
margin:11px 16px 11px 0;
border-radius:4px;
}
.rightInfo{
    flex:1;
    display: flex;
    justify-content: flex-end;
    align-items: center;
    float: right;
}
.rightInfo .arrow {
    width:18px;
    height: 18px;
}
.rightInfo .arrow image{
    width:18px;
    height: 18px;

    
}
.rightInfo .date{
    font-size:12px;
font-family:PingFangSC-Regular;
font-weight:400;
color:rgba(181,181,181,1);
line-height:17px;
line-height: 68px;
}

